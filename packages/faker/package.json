{
  "name": "@graphql-reveal/faker",
  "version": "0.0.2",
  "description": "Mock or extend your GraphQL Modules with faked data",
  "repository": "https://github.com/icy-design/graphql-reveal.git",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "pnpm run clean",
    "build": "tsc"
  },
  "dependencies": {
    "@graphql-toolkit/common": "0.10.7",
    "@graphql-toolkit/schema-merging": "0.10.7",
    "faker": "4.1.0",
    "graphql-tools": "5.0.0",
    "moment": "2.24.0"
  },
  "peerDependencies": {
    "@graphql-modules/core": "^0.7.17",
    "@graphql-modules/di": "^0.7.17",
    "graphql": "^14.1.1 || ^15.0.0"
  },
  "devDependencies": {
    "@types/faker": "4.1.6",
    "@types/graphql": "^14.2.3",
    "@types/node": "^11.9.4",
    "graphql-tag": "2.10.3",
    "typescript": "3.9.3"
  },
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "typings": "dist/index.d.ts",
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "buildOptions": {
    "input": "./src/index.ts"
